<div class="container">
  <div class="row">
    <div class="col-md-8 mt-2">
      <h2>カテゴリ編集</h2>
      <%= form_with model: @category, url: admin_category_path(@category.id), local: true do |f| %>
      <table class="table table-bordered table-sm">
            <thead class=""></thead>
              <tr><th>カテゴリ名</th><td><%= f.text_field :name %></td></tr>
      </table>
      <%= f.submit "変更を保存する", class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>