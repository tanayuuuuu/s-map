<div class="container">
  <div class="row">
    <div class="col-md-8 mt-2">
      <h1>ユーザー一覧</h1>
      <% @users.each do |user| %>
      <table class="table table-bordered table-sm">
            <thead class=""></thead>
              <tr><th>ユーザーID</th><td><%= user.id %></td></tr>
              <tr><th>氏名</th><td><%= user.name %></td></tr>
              <tr><th>住所</th><td><%= user.address %></td></tr>
              <td><%= link_to "編集する", edit_admin_user_path(user.id), class:"btn btn-success" %></td>
        </table>
        <% end %>
    </div>
  </div>
</div>