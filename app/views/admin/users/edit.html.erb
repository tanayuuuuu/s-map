<div class="container">
  <div class="row">
    <div class="col-md-8 mt-2">
       <h1>ユーザー編集</h1>
       <table class="table">
            <thead class=""></thead>
              <%= form_with model: @user, url: admin_user_path(@user) do |f| %>
              <tr><td><%= f.label :name, "名前" %>
              <%= f.text_field :name %></td></tr>
             <br/>
              <tr><td><%= f.label :name_kana, "フリガナ" %>
              <%= f.text_field :name_kana %></td></tr>
              <br/>
              <tr><td><%= f.label :email, "メールアドレス" %>
              <%= f.email_field :email %></td></tr>
              <br/>
              <tr><td><%= f.label :post_code, "郵便番号" %>
              <%= f.text_field :post_code %></td></tr>
              <br/>
              <tr><td><%= f.label :address, "住所" %>
              <%= f.text_field :address %></td></tr>
              <br/>
              <tr><td><%= f.label :phone_number, "電話番号" %>
              <%= f.text_field :phone_number %></td></tr>
              <td><%= f.submit "変更", class:"btn btn-success" %></td>
            </div>
            <% end %>
  </div>
</div>
